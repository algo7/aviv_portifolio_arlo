		<!-- Quote -->
		<div class="arlo_tm_section" id="inspiration">
			<div class="arlo_tm_testimonial">
				<div class="testimonial_inner">
					<div class="testimonial_bg">
						<div class="image jarallax" data-speed-="0" data-img-url="img/slider/1.jpg"></div>
						<div class="overlay"></div>
					</div>
					<div class="testimonial_content">
						<div class="container">
							<div class="content_inner">
								<div class="icon">
									<img class="svg" src="img/svg/quotes.svg" alt="" />
								</div>
								<div class="texts">
									<p style='
												width:80%;margin:auto;
												font-family: "Montserrat";'>
										<strong>{{quote}}
										</strong>
									</p>
									<br>

									<div class="author">
										<div class="image">
											<div class="main" data-img-url="{{imgUrl}}"></div>
										</div>
										<div class=" job">
											<h3 style="padding-bottom: 20px;">{{author}}</h3>
											<span>
												{{description}}
											</span>
										</div>
									</div>
								</div>

							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- /TESTIMONIAL -->

		<!-- TIMELINE -->
		<div class="arlo_tm_section" id="experience">
			<div class="arlo_tm_timeline">
				<div class="container">
					<div class="arlo_title_holder">
						<span>Working Process</span>
						<h3>Experience</h3>
					</div>

					<div class="timeline_inner">

						<div class="left">
							<ul>
								{{#each experienceLeft}}
								<li>
									<div class="inner">
										<img class="svg" src="{{typeImageUrl}}" alt="" />
										<p class="year">
											{{this.year}} @
											<a class="{{this.hrefClass}}" href="#" style="color:white">
												{{this.location}}
											</a>
										</p>
										<h3 class="title">{{this.position}}</h3>
										<p class="text">
											{{this.description}}
										</p>
									</div>
								</li>
								{{/each}}
							</ul>
						</div>

						<div class="right">
							<ul>
								{{#each experienceRight}}
								<li>
									<div class="inner">
										<img class="svg" src="{{typeImageUrl}}" alt="" />
										<p class=" year">{{this.year}} @
											<a class="{{this.hrefClass}}" href="{{this.link}}" style="color:white">
												{{this.location}}
											</a>
										</p>
										<h3 class="title">{{this.position}}</h3>
										<p class="text">
											{{this.description}}
										</p>
									</div>
								</li>
								{{/each}}
							</ul>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- /TIMELINE -->


		<!-- CONTACT -->
		<div class="arlo_tm_section" id="contact">
			<div class="arlo_tm_contact">
				<div class="container">
					<div class="arlo_title_holder">
						<span>Get in Touch</span>
						<h3>Contact</h3>
					</div>
					<div class="contact_inner">
						<div class="short_list">
							<ul>
								<li>
									<img class="svg" src="img/svg/location-1.svg" alt="" />
									<h3>Location</h3>
									<p>{{user.location}}</p>
								</li>
								<li>
									<img class="svg" src="img/svg/message-3.svg" alt="" />
									<h3>Email</h3>
									<p>{{user.email}}</p>
								</li>
								<li>
									<img class="svg" src="img/svg/call-2.svg" alt="" />
									<h3>Phone</h3>
									<p>{{user.phone}}</p>
								</li>
							</ul>
						</div>

					</div>
				</div>
			</div>
		</div>
		<!-- /CONTACT -->

		<!-- FOOTER -->
		<div class="arlo_tm_footer">
			<div class="container">
				<div class="footer_inner">
					<div class="top">
						<a class="arlo_tm_totop" href="#"></a>
					</div>
					<div class="copy">

					</div>
					<div class="top">
						<a class="arlo_tm_totop" href="#"></a>
					</div>
				</div>
			</div>
		</div>
		<!-- /FOOTER -->

	</div>
</div>
<!-- /RIGHTPART -->