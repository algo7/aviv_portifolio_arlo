<!-- RIGHTPART -->
<div class="arlo_tm_rightpart">
	<div class="rightpart_inner">

		<!-- HERO -->
		<div class="arlo_tm_section" id="home">
			<div class="arlo_tm_hero_header">
				<div class="hero_image jarallax" data-img-url="img/slider/7.jpg" data-speed="0"></div>
				<div class="overlay"></div>
				<div class="hero_content">
					<div class="image_wrap">
						<img src="img/about/500x500.jpg" alt="" />
						<div class="main" data-img-url="{{user.profileImg}}"></div>
					</div>
					<div class="name_holder">
						<h3>{{user.firstName}} <span>{{user.lastName}}</span></h3>
					</div>
					<div class="text_typing">
						<p>I'm a <span class="arlo_tm_animation_text_word"></span></p>
					</div>
				</div>
				<div class="arlo_tm_arrow_classic bounce anchor">
					<a href="#about"><i class="xcon-angle-double-down"></i></a>
				</div>
			</div>
		</div>
		<!-- /HERO -->

		<!-- ABOUT -->
		<div class="arlo_tm_section" id="about">
			<div class="arlo_tm_about_wrap_all">
				<div class="arlo_tm_about">
					<div class="container">
						<div class="about_inner">
							<div class="leftbox">
								<div class="about_image_wrap parallax" data-relative-input="true">
									<div class="image layer" data-depth="0.1">
										<img src="img/about/550x650.jpg" alt="" />
										<div class="inner" data-img-url="img/about/2.jpg"></div>
									</div>
									<div class="border layer" data-depth="0.2">
										<img src="img/about/550x650.jpg" alt="" />
										<div class="inner"></div>
									</div>
								</div>
							</div>
							<div class="rightbox">
								<div class="arlo_title_holder about">
									<span>Main Info</span>
									<h3>About Me</h3>
								</div>
								<div class="text">
									<p>My name is <span>{{user.firstName}} {{user.lastName}}</span>.
										{{user.bio}}
									</p>
								</div>
								<div class="about_short_contact_wrap">
									<ul>
										<li>
											<img class="svg" src="img/svg/gift.svg" alt="" />
											<span><label>Birthday:</label> {{user.birthDay}}</span>
										</li>
										<li>
											<img class="svg" src="img/svg/calendar-4.svg" alt="" />
											<span>
												<label>Birth Name:</label>
												{{user.birthName}}
											</span>
										</li>
										<li>
											<img class="svg" src="img/svg/location-1.svg" alt="" />
											<span><label>Location:</label> <a class="href_location"
													href="#">{{user.location}}</a></span>
										</li>
										<li>
											<img class="svg" src="img/svg/game-controller-2.svg" alt="" />
											<span>
												<label>Interests:</label>
												{{user.interests}}
											</span>
										</li>
										<li>
											<img class="svg" src="img/svg/call-2.svg" alt="" />
											<span><label>Phone:</label> <a
													href="tel:{{user.phone}}">{{user.phone}}</a></span>
										</li>
										<li>
											<img class="svg" src="img/svg/study-5.svg" alt="" />
											<span>
												<label>Study:</label>
												<a class="href_location" href="#">
													{{user.study}}</a>
											</span>
										</li>
										<li>
											<img class="svg" src="img/svg/message-3.svg" alt="" />
											<span><label>Mail:</label> <a
													href="mailto:{{user.email}}">{{user.email}}</a></span>
										</li>
										<li>
											<img class="svg" src="img/svg/degree-1.svg" alt="" />
											<span><label>Degree:</label>{{user.degree}}</span>
										</li>
									</ul>
								</div>

								{{!-- <div class="buttons">
												<ul>
													<li><a href="#"><span>Download CV</span></a></li>
													<li><a href="#"><span>Send Message</span></a></li>
												</ul>
											</div> --}}
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
		<!-- /ABOUT -->


